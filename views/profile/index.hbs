
<h1>Name</h1>
<a href="../posts">Timeline</a>
<form action="/sessions?_method=DELETE" method="POST">
  <input type="submit" value="Log Out" />
</form>

<div>
  <p>Friends</p>
  <ul class="users">
    {{#each friends_names}}
      <form action="/" method="POST"></form>
      <li>{{this.firstName}} {{this.lastName}}<input type="submit" value="Remove Friend" />
      </form>
      {{!-- <li>{{this}}</li> --}}
    {{/each}}
  </ul>


</div>

<div>
  <p>Users</p>
  <ul class="users">
    {{#each nonFriends}}
      {{!-- <li>{{this.email}}</li> --}}
      <form action="/" method="POST"></form>
      <li>{{this.firstName}} {{this.lastName}}<input type="submit" value="Add Friend" />
      </form>
    {{/each}}
  </ul>

</div>