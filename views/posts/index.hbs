<form action='/sessions?_method=DELETE' method='POST'>
      <input type="submit" value="Log Out">
    </form>
<h1>Timeline</h1>
<ul class="posts">
  {{#each posts}}
  <li><a href="/posts/{{this._id}}">{{this.message}}</a></li> 
  <li>{{this.likes}} likes</li>
  {{/each}}
</ul>


<a class="new-post-link" href="/posts/new">New post</a>
